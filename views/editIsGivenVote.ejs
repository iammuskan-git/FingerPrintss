<%- include('./includes/header') %>

<div class="container">
    <div class="row">
        <div class="col-lg-6 mx-auto mt-4">
           <div class="card shadow">
            <div class="card-header bg-success">
                <h3 class="text-light">Edit IsGivenVote (<%= user.id %>) </h3>
            </div>
            <div class="card-body p-4">
                <form action="/updatess/<%= user.id %>" method="post" id="add-form" enctype="multipart/form-data">
        
                <div class="mb-3">
                    <label for="voter_id">Voter_ID</label>
                    <input type="text" name="voter_id"  class="form-control form-control-lg" value="<%= user.voter_id %>" required/>
                </div>
                <div class="mb-3 d-grid">
                    <input type="submit" name="submit" value="Update IsGivenVote" class="btn btn-success btn-lg">
                </div>
                </form>
            </div>
           </div>
            
        </div>
    </div>
</div>
<%- include('./includes/footer') %>